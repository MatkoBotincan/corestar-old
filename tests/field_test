Implication:  
  | |- |
? False


Implication:  
  | field(nil(),f,x) |- | field(nil(),f2,y)  
? True

Implication:  
  | field(x,f,x) |- | field(nil(),f2,y)  
? False

Implication:  
  | field(x,f,x) |- | field(x,f,y)  
? False

Implication:  
 x=y | field(x,f,x) |- | field(x,f,y)  
? True

Implication:  
 | field(x,f,x) * (x=y| ) || (x=nil() | ) |- | field(x,f,y)  
? True


Inconsistency:
 | field(x,f,y) * field(x,f,z)
? True

Inconsistency:
 | field(x,f,y) * field(x,f2,z)
? False

Inconsistency:
 | field(x,f,y) * field(y,f2,z)
? False


Implication: 
 | field(x,f,y) |- x!=nil() |
? True

Implication: 
 | ( x!=nil() |  ) || (| field(x,f,y)) |- x!=nil() |
? True

Implication: 
 y!=nil() | ( x!=nil() |  ) || ( x=y | ) |- x!=nil() |
? True

Implication: 
 y!=nil() | ( x!=nil() |  ) || ( x=y | field(y,f,j) ) |- x!=nil() |
? True


